    "karma-junit-reporter": "1.2.0",
    "karma-polyfill": "1.0.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "3.0.0",
    "mkdirp": "0.5.1",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "1.3.0",
    "morgan": "1.9.1",
    "nightwatch": "^1.0.14",
    "nightwatch-html-reporter": "^2.0.6",
    "node-sass": "4.9.2",
    "open-browser-webpack-plugin": "0.0.5",
    "pretty": "2.0.0",
    "puppeteer": "1.4.0",
