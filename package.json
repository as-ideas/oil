    "nightwatch": "0.9.21",
    "nightwatch-html-reporter": "2.0.5",
    "node-sass": "4.8.3",
    "open-browser-webpack-plugin": "0.0.5",
    "pretty": "2.0.0",
    "puppeteer": "1.4.0",
    "raw-loader": "0.5.1",
    "remap-istanbul": "0.11.0",
    "rimraf": "2.6.2",
    "sass-loader": "7.0.1",
    "selenium-download": "2.0.12",
