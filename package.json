    "jasmine": "3.1.0",
    "jasmine-ajax": "3.3.1",
    "jasmine-core": "3.1.0",
    "jasmine-jquery": "2.1.1",
    "jasmine-node": "^2.0.1",
    "json-loader": "0.5.7",
    "karma": "^3.1.3",
    "karma-babel-preprocessor": "7.0.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage": "1.1.2",
    "karma-coveralls": "1.2.1",
    "karma-es6-shim": "1.0.0",
